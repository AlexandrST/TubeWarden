a.list-group-item.list-video-item(href='/video/' + video.videoId )
  span.video-title= video.title
  ul.list-inline.list-video-item-info
    if video.deletedAt
      li Удалено с YouTube: 
        i.date-time-value(data-value=video.deletedAt)
    else
      li В трендах: 
        i.date-time-value(data-value=video.createdAt)
        i  - 
        i.date-time-value(data-value=video.trendsAt)
    li
      span.glyphicon.glyphicon-eye-open(aria-hidden="true" )
      i= video.viewCount
    li
      span.glyphicon.glyphicon-thumbs-up(aria-hidden="true" style=(video.violationIndexLike > 0? "color:#00b757;" : "" ) )
      i= video.likeCount
    li
      span.glyphicon.glyphicon-thumbs-down(aria-hidden="true" style=(video.violationIndexDislike > 0? "color:#f30000;" : "" ))
      i= video.dislikeCount



      