{
  "name": "youtube_warden",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/site/server.js",
    "grabber": "node ./bin/script/backgroundWorker.js",
    "clean": "del-cli bin/",
    "copy": "cpx \"src/**/*.{pug,png}\" bin",
    "build:core": "tsc --project ./src/tsconfig.json",
    "build:client": "tsc --project ./src/site/public/tsconfig.json",
    "build:css": "node-sass --include-path ./src/site/public/scss ./src/site/public/scss/main.scss   ./bin/site/public/stylesheets/main.css",
    "build": "npm run clean && npm run copy && npm run build:core && npm run build:client&& npm run build:css"
  },
  "dependencies": {
    "@types/chart.js": "^2.7.37",
    "@types/express": "^4.16.0",
    "@types/highcharts": "^5.0.32",
    "@types/jquery": "^3.3.17",
    "@types/node": "^10.11.7",
    "apicache": "^1.2.3",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "cpx": "^1.5.0",
    "debug": "~4.1.0",
    "del": "^3.0.0",
    "del-cli": "^1.1.0",
    "express": "^4.16.4",
    "fs": "0.0.1-security",
    "googleapis": "^34.0.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.1",
    "node-sass": "^4.9.3",
    "node-schedule": "^1.3.0",
    "path": "^0.12.7",
    "pug": "2.0.3",
    "reflect-metadata": "^0.1.12",
    "rfr": "^1.2.3",
    "sequelize": "^4.39.0",
    "sequelize-typescript": "^0.6.6",
    "serve-favicon": "^2.5.0",
    "snowball-stemmer.jsx": "^0.2.3",
    "typescript": "^3.1.3"
  }
}
