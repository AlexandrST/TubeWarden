{
  "name": "youtube_warden",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/site/server.js",
    "grabber": "node ./bin/script/backgroundWorker.js",
    "clean": "del-cli bin/",
    "copy": "cpx \"src/**/*.{pug,png}\" bin",
    "build:core": "tsc --project ./src/tsconfig.json",
    "build:client": "tsc --project ./src/site/public/tsconfig.json",
    "build:css": "node-sass --include-path ./src/site/public/scss ./src/site/public/scss/main.scss   ./bin/site/public/stylesheets/main.css",
    "build": "npm run clean && npm run copy && npm run build:core && npm run build:client&& npm run build:css"
  },
  "dependencies": {
    "@types/chart.js": "^2.7.3",
    "@types/express": "^4.11.0",
    "@types/jquery": "^3.2.17",
    "@types/node": "^8.5.8",
    "apicache": "^1.2.0",
    "bluebird": "^3.5.1",
    "body-parser": "~1.18.2",
    "compression": "^1.7.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "cpx": "^1.5.0",
    "debug": "~2.6.9",
    "del": "^3.0.0",
    "express": "^4.16.2",
    "fs": "0.0.1-security",
    "googleapis": "^23.0.0",
    "moment": "^2.20.1",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.1",
    "node-sass": "^4.7.2",
    "node-schedule": "^1.3.0",
    "path": "^0.12.7",
    "pug": "2.0.0-beta11",
    "reflect-metadata": "^0.1.10",
    "rfr": "^1.2.3",
    "sequelize": "^4.30.2",
    "sequelize-typescript": "^0.6.2",
    "serve-favicon": "^2.4.5",
    "snowball-stemmer.jsx": "^0.2.3",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "del-cli": "^1.1.0"
  }
}
